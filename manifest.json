{
  "manifest_version": 3,
  "name": "gondolin",
  "version": "1.0",
  "description": "another AI app",
  "action": {},
  "permissions": [
    "sidePanel",
    "tabs",
    "scripting",
    "storage",
    "downloads",
    "fileSystem",
    "fileSystem.write"
  ],
  "host_permissions": ["<all_urls>"],
  "background": {
    "service_worker": "background.js"
  },
  "side_panel": {
    "default_path": "popup.html"
  },
  "web_accessible_resources": [{
    "resources": ["fullpage.html", "fullpage.css", "fullpage.js"],
    "matches": ["<all_urls>"]
  }]
}
